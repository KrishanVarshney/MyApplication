    package="com.example.v.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowData" />
        <activity android:name=".admin_profile" />
        <activity android:name=".student_profile" />
        <activity android:name=".MainActivity" />
        <activity android:name=".add_admin" />
        <activity
            android:name=".show_student_data"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".edit_student_data" />
        <activity android:name=".show_admin_data" />
        <activity android:name=".notice" />
        <activity android:name=".ShowStudentsNotice"></activity>
    </application>

</manifest>